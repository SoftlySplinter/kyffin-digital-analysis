    doi = {10.1109/iccv.2001.937552},
    institution = {Dept. of Electr. Eng. \& Comput. Sci., California Univ., Berkeley, CA, USA},
    isbn = {0-7695-1143-0},
    journal = {Computer Vision, 2001. ICCV 2001. Proceedings. Eighth IEEE International Conference on},
    pages = {454--461 vol.1},
    posted-at = {2013-03-05 14:32:29},
    priority = {2},
    publisher = {IEEE},
    title = {Matching shapes},
    t_url = {http://dx.doi.org/10.1109/iccv.2001.937552},
